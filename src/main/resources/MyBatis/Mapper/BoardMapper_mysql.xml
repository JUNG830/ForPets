<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- mapper 파일 경로 입력  -->
<mapper namespace="ForPets.Mapper.BoardMapper">
    <select id="Board_seq" parameterType="Long" >
        select *
        from Board
        where seq = #{id};
    </select>

</mapper>